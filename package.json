{
  "name": "church-plus-manager",
  "version": "1.0.0",
  "description": "Sistema de gerenciamento para igrejas - Neon Database + Netlify",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "npx vite build",
    "build:server": "esbuild server/index.ts --bundle --platform=node --target=node18 --format=esm --outfile=dist-server/index.js --packages=external",
    "start:server": "NODE_ENV=production node dist-server/index.js",
    "build:full": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives",
    "lint:strict": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,md}\"",
    "prepare": "husky",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:client": "vitest run",
    "test:client:watch": "vitest",
    "test:client:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:e2e",
    "migrate-to-neon": "NODE_ENV=development tsx -e \"import('./server/migrateToNeon.js').then(m => m.migrateToNeon())\"",
    "migrate-extradata": "NODE_ENV=development tsx server/migrateExtraDataToColumns.ts",
    "migrate-roles": "NODE_ENV=development tsx server/migrateRoles.ts",
    "recalcular-campos-vazios": "NODE_ENV=development tsx server/recalcularCamposVazios.ts",
    "commit": "node scripts/commit.js",
    "deploy": "npm run build && npx netlify deploy --prod --message \"$(git log -1 --pretty=%B)\"",
    "deploy:preview": "npm run build && npx netlify deploy",
    "deploy:status": "bash scripts/deploy-status.sh",
    "status": "npx netlify status",
    "logs": "npx netlify logs:function api",
    "open:netlify": "open https://app.netlify.com/sites/meu7care/deploys",
    "open:prod": "open https://meu7care.netlify.app/",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "size-limit": "size-limit",
    "analyze": "ANALYZE=true npm run build",
    "stryker": "stryker run"
  },
  "size-limit": [
    {
      "path": "dist/assets/js/vendor-react-*.js",
      "limit": "150 KB"
    },
    {
      "path": "dist/assets/js/vendor-ui-*.js",
      "limit": "200 KB"
    },
    {
      "path": "dist/assets/js/index-*.js",
      "limit": "100 KB"
    }
  ],
